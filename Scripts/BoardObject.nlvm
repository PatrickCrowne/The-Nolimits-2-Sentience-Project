import com.nolimitscoaster.*;

public class BoardObject extends Script{
	
	private SceneObject sco;
	private static int id = 0;

	public bool onInit(){
		
		sco = sim.getSceneObjectForEntityId(getParentEntityId());
		NodeNetwork.addNode(new Node(sco, NodeNetwork.nodeCount, Node.BOARD));
		id++;
	
		return true;
		
	}
	
}